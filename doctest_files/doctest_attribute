"""
>>> from module_builder.attribute import Attribute
>>> a = Attribute('first_attribute', 'string')
>>> print(a.name)
first_attribute
>>> print(a.type)
str
>>> b = Attribute('first_attribute', 'int')
>>> print(b.type)
int
>>> c = Attribute('first_attribute', 'list')
>>> print(c.type)
list
>>> d = Attribute('first_attribute', 'tuple')
>>> print(d.type)
tuple

"""